{
  "options": {
    "chart": {
      "height": 390,
      "type": "radialBar"
    },
    "title": {
      "text": "Auto Renewal Percentages",
      "align": "left",
      "style": {
        "color": "#ccc"
      }
    },
    "plotOptions": {
      "radialBar": {
        "offsetY": 0,
        "startAngle": 0,
        "endAngle": 270,
        "hollow": {
          "margin": 5,
          "size": "30%",
          "background": "transparent"
        },
        "dataLabels": {
          "name": {
            "show": false
          },
          "value": {
            "fontSize": "24px",
            "offsetY": 7,
            "color": "#ccc"
          }
        }
      }
    },
    "colors": ["#39e6a6", "#fdc55d", "#fe7b8e", "#0090fa"],
    "legend": {
      "show": true,
      "floating": true,
      "fontSize": "16px",
      "position": "left",
      "offsetX": 30,
      "offsetY": 30,
      "labels": {
        "useSeriesColors": true
      },
      "markers": {
        "size": 0
      },
      "itemMargin": {
        "vertical": 3
      }
    },
    "responsive": [
      {
        "breakpoint": 480,
        "options": {
          "legend": {
            "show": false
          }
        }
      }
    ]
  }
}
